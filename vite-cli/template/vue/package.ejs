{
  "name": "vite-template",
  "private": true,
  "version": "1.0.0",
  <% if (tinyVue) { -%>
  "type": "module",
  <% } -%>
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    <% if (useEslint) { -%>
    <%- EslintScript %>
    <% } -%>
    <% if (usePrettier) { -%>
    <%- PrettierScript %>
    <% } -%>
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    <%- constantProDeps %>
    <% if (useTheme) { -%>
    <%- themeEffect %>
    <% if (elementPlus || useTheme) { -%>
    <%- elementPlusProThemeEffect %>
    <% } -%>
    <% } -%>
    <% if (useRouter) { -%>
    <%- Router %>
    <% } -%>
    <%- ui %>
    <% if (varlet) { -%>
    <%- varletEffect %>
    <% } -%>
    <% if (usePinia || useTheme) { -%>
    <%- Pinia %>
    <% } -%>
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    <% if (useTheme && elementPlus) { -%>
    <%- elementPlugThemeEffect %>
    <% } -%>
    <% if (precss === 'scss' || useTheme && components !== 'antDesignVue' ) { -%>
    <%- sassEffect %>
    <% } -%>
    <% if (precss === 'less' || useTheme && components === 'antDesignVue') { -%>
    <%- lessEffect %>
    <% } -%>
    <% if (tinyVue) { -%>
    <%- tinyVueEffect %>
    <% } -%>
    <% if (vuetify) { -%>
    <%- vuetifyEffect %>
    <% } -%>
    <% if (useEslint) { -%>
      <%- Eslint %>
    <% } -%>
    <% if (usePrettier) { -%>
      <%- Prettier %>
    <% } -%>
    <% if (usePrettier && useEslint) { -%>
      <%- EslintWithPrettierScript %>
    <% } -%>
    "@vitejs/plugin-vue": "^4.0.0",
    <% if (pluginList.length > 0) { -%>
      <%- pluginList %>
    <% } -%>
    <%- constantDevDeps %>
  }
}
