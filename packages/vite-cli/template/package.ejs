{
  "name": "test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    <% if (Eslint) { -%>
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    <% } -%>
    <% if (Prettier) { -%>
    "prettier": "prettier --write ./**/*.{html,vue,ts,js,json,md}"
    <% } -%>
  },
  "dependencies": {
    "@vueuse/core": "^8.9.1",
    "@vueuse/head": "^0.7.6",
    "@types/node": "^16.11.43",
    "axios": "^0.27.2",
    <% if (Router) { -%>
      "vue-router": "^4.1.1",
    <% } -%>
    <% if (components === 'element-plus') { -%>
      "element-plus": "^2.2.5",
    <% } -%>
    <% if (components === 'naive-ui') { -%>
      "naive-ui": "^2.31.0",
    <% } -%>
    <% if (components === 'ant-design') { -%>
      "ant-design-vue": "3.2.1",
    <% } -%>
    "vue": "^3.2.25",
    <% if (Pinia) { -%>
      "pinia": "^2.0.14",
      "pinia-plugin-persist": "^1.0.0"
    <% } -%>
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7",
    <% if (precss === 'scss') { -%>
      "sass": "1.50.1",
    <% } -%>
    <% if (Eslint) { -%>
      "eslint": "^8.18.0",
      "eslint-plugin-vue": "^9.1.1",
      "@typescript-eslint/eslint-plugin": "^5.30.5",
      "@typescript-eslint/parser": "^5.30.5",
    <% } -%>
    <% if (Prettier) { -%>
      "prettier": "^2.7.1",
    <% } -%>
    <% if (Prettier && Eslint) { -%>
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.2.1",
    <% } -%>
    <% if (plugins.indexOf('jsx') !== -1) { -%>
      "@vitejs/plugin-vue-jsx": "^1.3.10",
    <% } -%>
    <% if (plugins.indexOf('legacy') !== -1) { -%>
      "@vitejs/plugin-legacy": "^1.8.2",
    <% } -%>
    <% if (plugins.indexOf('vue-components') !== -1) { -%>
      "unplugin-vue-components": "^0.21.1",
    <% } -%>
    <% if (plugins.indexOf('auto-import') !== -1) { -%>
      "unplugin-auto-import": "^0.9.2",
    <% } -%>
    <% if (plugins.indexOf('unocss') !== -1) { -%>
      "unocss": "^0.42.0",
    <% } -%>
    <% if (plugins.indexOf('pwa') !== -1) { -%>
      "vite-plugin-pwa": "^0.12.2",
    <% } -%>
    <% if (plugins.indexOf('visualizer') !== -1) { -%>
      "rollup-plugin-visualizer": "^5.6.0",
    <% } -%>
    <% if (plugins.indexOf('icons') !== -1) { -%>
      "unplugin-icons": "^0.14.7",
      "@iconify/json": "^2.1.74",
    <% } -%>
    <% if (plugins.indexOf('inspect') !== -1) { -%>
      "vite-plugin-inspect": "^0.5.1"
    <% } -%>
  }
}
