{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:ignore": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    <% if (useEslint) { -%>
    <%- EslintScript %>
    <% } -%>
    <% if (usePrettier) { -%>
    <%- PrettierScript %>
    <% } -%>
  },
  "dependencies": {
    "@vueuse/core": "^8.9.1",
    "@vueuse/head": "^0.7.6",
    "@types/node": "^16.11.43",
    "axios": "^0.27.2",
    <% if (useTheme) { -%>
    "@erkelost/colorpicker": "^1.0.8",
    "@erkelost/layout": "2.9.3",
    "@erkelost/utils": "^1.0.2",
    <% if (elementPlus) { -%>
    "css-color-function": "^1.3.3",
    "colord": "^2.9.2",
    <% } -%>
    <% } -%>
    <% if (useRouter) { -%>
    <%-  Router %>
    <% } -%>
    <%- ui %>
    <% if (varlet) { -%>
      "@varlet/touch-emulator": "^1.27.20",
    <% } -%>
    "vue": "^3.2.27",
    <% if (usePinia || useTheme) { -%>
    <%- Pinia %>
    <% } -%>
  },
  "devDependencies": {
    <% if (useTheme) { -%>
      "@pureadmin/theme": "^2.0.0",
    <% } -%>
    "vite": "^4.0.1",
    <% if (precss === 'scss' || useTheme && components !== 'antDesignVue' ) { -%>
      "sass": "^1.53.0",
    <% } -%>
    <% if (precss === 'less' || useTheme && components === 'antDesignVue') { -%>
      "less": "^4.1.3",
    <% } -%>
    <% if (vuetify) { -%>
      "vite-plugin-vuetify": "^1.0.0",
    <% } -%>
    <% if (Eslint) { -%>
      <%- Eslint %>
    <% } -%>
    <% if (usePrettier) { -%>
      <%- Prettier %>
    <% } -%>
    <% if (usePrettier && useEslint) { -%>
      <%- EslintWithPrettierScript %>
    <% } -%>
    "@vitejs/plugin-vue": "^3.2.0",
    "vite-plugin-pwa": "^0.14.0",
    <%- pluginList %>
    "typescript": "^4.9.3",
    "vue-tsc": "^1.0.9"
  }
}
