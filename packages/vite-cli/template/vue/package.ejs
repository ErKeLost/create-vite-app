{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    <% if (useEslint) { -%>
    <%- EslintScript %>
    <% } -%>
    <% if (usePrettier) { -%>
    <%- PrettierScript %>
    <% } -%>
  },
  "dependencies": {
    "@vueuse/core": "^8.9.1",
    "@vueuse/head": "^0.7.6",
    "@types/node": "^16.11.43",
    "axios": "^0.27.2",
    <% if (useTheme) { -%>
    "@erkelost/colorpicker": "^1.0.8",
    "@erkelost/layout": "2.9.3",
    "@erkelost/utils": "^1.0.2",
    "css-color-function": "^1.3.3",
    "colord": "^2.9.2",
    <% } -%>
    <% if (useRouter) { -%>
    <%-  Router %>
    <% } -%>
    <%- ui %>
    "vue": "^3.2.27",
    <% if (usePinia || useTheme) { -%>
    <%- Pinia %>
    <% } -%>
  },
  "devDependencies": {
    <% if (useTheme) { -%>
      "@pureadmin/theme": "^2.0.0",
    <% } -%>
    "vite": "^3.0.4",
    <% if (precss === 'scss' || useTheme) { -%>
      "sass": "^1.53.0",
    <% } -%>
    <% if (components === 'vuetify') { -%>
      "vite-plugin-vuetify": "^1.0.0-alpha.13",
    <% } -%>
    <% if (Eslint) { -%>
      <%- Eslint %>
    <% } -%>
    <% if (usePrettier) { -%>
      <%- Prettier %>
    <% } -%>
    <% if (usePrettier && useEslint) { -%>
      <%- EslintWithPrettierScript %>
    <% } -%>
    "@vitejs/plugin-vue": "^3.0.1",
    <%- pluginList %>
    "typescript": "^4.7.4",
    "vue-tsc": "^0.39.0"
  }
}
